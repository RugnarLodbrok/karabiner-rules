{
  "title": "Exchange Command+Tab (generally 'cycle through applications') and Control+Tab (generally 'cycle through tabs') my",
  "rules": [
    {
      "description": "win-like ctrl+tab",
      "manipulators": [
        {
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": [
                "control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "win-like alt+tab",
      "manipulators": [
        {
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": [
                "option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "control"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
