{
  "title": "Windows-like",
  "rules": [
    {
      "description": "swap left control/command keys",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_control",
            "modifiers": {"optional": "any"}
          },
          "to": {
            "key_code": "left_command"
          }
        },
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {"optional": "any"}
          },
          "to": {
            "key_code": "left_control"
          }
        }
      ]
    },
    {
      "description": "home",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "home",
            "modifiers": {
              "optional": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": "command"
            }
          ]
        }
      ]
    },
    {
      "description": "end",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "end",
            "modifiers": {
              "optional": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": "command"
            }
          ]
        }
      ]
    },
    {
      "description": "ctrl+tab",
      "manipulators": [
        {
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "control"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "alt+tab",
      "manipulators": [
        {
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": [
                "option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "ctrl+left",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_arrow",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "caps_lock",
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": "option"
            }
          ]
        }
      ]
    },
    {
      "description": "ctrl+right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_arrow",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "caps_lock",
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": "option"
            }
          ]
        }
      ]
    },
    {
      "description": "shift+ctrl+left",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_arrow",
            "modifiers": {
              "mandatory": [
                "shift",
                "command"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "option",
                "shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "shift+ctrl+right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_arrow",
            "modifiers": {
              "mandatory": [
                "shift",
                "command"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "option",
                "shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "ctrl+backspace",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "caps_lock",
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace",
              "modifiers": "option"
            }
          ]
        }
      ]
    },
    {
      "description": "ctrl+insert",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "insert",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "key_code": "c",
              "modifiers": "command"
            }
          ]
        }
      ]
    },
    {
      "description": "shift+insert",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "insert",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "v",
              "modifiers": "command"
            }
          ]
        }
      ]
    },
    {
      "description": "pause_break_to_combination",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "pause"
          },
          "to": [
            {
              "key_code": "keypad_asterisk",
              "modifiers": [
                "right_control",
                "right_shift"
              ]
            }
          ]
        }
      ]
    }
  ]
}
